<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelpoint">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravelPoint"
        android:usesCleartextTraffic="true">
        <activity android:name=".e_reference"></activity>
        <activity android:name=".e_privacy_policy" />
        <activity android:name=".e_delete_acc" />
        <activity android:name=".e_updatePassword" />
        <activity android:name=".e_updateUsername" />
        <activity android:name=".d_explore_focus" />
        <activity android:name=".e_details_page" />
        <activity android:name=".d_home_page" />
        <activity android:name=".c_signup_page" />
        <activity android:name=".b_login_page" />
        <activity android:name=".a_start_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>